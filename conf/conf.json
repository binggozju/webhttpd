{
	"common": {
		"worker_processes": 2,
		"max_connection": 1024
	},

	"log": {
		"level": "info",
		"maxsize": 1,
		"backup": 3,
		"runtime_log": "./logs/webhttpd.log",
		"access_log": "./logs/access.log"
	},

	"http": {
		"keepalive_timeout": 65
	},
	
	"server": {
		"website1": {
			"listen": 80
		},
		"website2": {
			"listen": 8080
		}
	}

}
